{
  "name": "Password hasher",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Hashes password fields automatically.",
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": [
        "other/jquery/jquery-3.3.1.min.js",
        "other/sjcl/sjcl.js",
        "lib.js",
        "keypresses.js",
        "content.js"
      ]
    }
  ]
}
